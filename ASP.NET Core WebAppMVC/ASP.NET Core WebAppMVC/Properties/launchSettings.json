{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  // IIS WEB Server settings
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:45616",
      "sslPort": 0
    }
  },

  // Kesteral Web server settings
  "profiles": {
    "http": {
      "commandName": "Project",
